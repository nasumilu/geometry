{
    "name": "nasumilu/geometry",
    "description": "Component provides the basic geometry classes used in Geographical Information Systems (GIS).",
    "type": "library",
    "homepage": "https://nasumilu.net",
    "license": "Apache-2.0",
    "keywords": ["gis",
        "feature",
        "geometry", 
        "point",
        "linestring",
        "polygon",
        "multipoint",
        "multipolygon",
        "geometrycollection",
        "tin",
        "triangle",
        "polyhedralsurface",
        "wkt",
        "wkb",
        "well-known text",
        "well-known binary"
    ],
    "authors": [
        {
            "name": "Michael Lucas",
            "email": "nasumilu@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 | 8.0",
        "symfony/serializer": "^v5.3|^v6.0",
        "doctrine/lexer": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Nasumilu\\Spatial\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nasumilu\\Spatial\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.3"
    }
}
