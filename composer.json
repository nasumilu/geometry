{
    "name": "nasumilu/geometry",
    "description": "Component provides the basic geometry classes used in Geographical Information Systems (GIS).",
    "type": "library",
    "homepage": "https://nasumilu.net",
    "license": "Apache-2.0",
    "keywords": ["gis", 
        "geometry", 
        "point",
        "linestring",
        "polygon",
        "multipoint",
        "multipolygon",
        "geometrycollection",
        "tin",
        "triangle",
        "polyhedralsurface"
    ],
    "authors": [
        {
            "name": "Michael Lucas",
            "email": "nasumilu-owner@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Nasumilu\\Spatial\\Geometry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nasumilu\\Spatial\\Tests\\Geometry\\": "tests/"
        },
        "files": ["src/static_cast.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.3"
    }
}
